   137                              <1> ; Открытие файла для чтения, записи
   138                              <1> %macro  FileOpen   3
   139                              <1>     section .data
   140                              <1>         %%rw  db  %2,0          ; признак доступа
   141                              <1>     section .text               ; the printf arguments
   142                              <1>         mov     rdi, %1         ; адрес строки открываемого файла
   143                              <1>         lea     rsi, [%%rw]       ; строка определяющая чтение-запись. Задается явно
